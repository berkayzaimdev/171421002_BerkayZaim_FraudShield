              {key}:
            </Typography>
            {typeof value === 'object' && value !== null ? (
              renderConfigSection(value, level + 1)
            ) : (
              <Typography 
                variant="body2" 
                sx={{ 
                  ml: 2, 
                  color: typeof value === 'number' ? '#d32f2f' : '#2e7d32',
