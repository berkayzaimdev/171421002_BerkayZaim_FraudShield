      </Box>

      {/* Model Listesi Tab */}
      <TabPanel value={activeTab} index={0}>
        {/* Filtreleme ve Arama AlanlarÄ± */}
        <Card variant="outlined" sx={{ mb: 3 }}>
          <CardContent>
